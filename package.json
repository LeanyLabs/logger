{
  "name": "@leanylabs/logger",
  "version": "1.2.0",
  "description": "LeanyLabs Logger",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "test": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "format": "prettier --config .prettierrc.js --write ."
  },
  "author": "LeanyLabs",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^14.11.2",
    "gts": "^3.1.1",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "typescript": "~4.7.0"
  },
  "repositiry": {
    "type": "git",
    "url": "git+https://github.com/LeanyLabs/logger.git"
  },
  "dependencies": {
    "winston": "^3.3.4",
    "winston-console-format": "^1.0.8"
  },
  "files": [
    "lib/**/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/tests/**/*.spec.+(ts|tsx|js)"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testEnvironment": "node",
    "maxConcurrency": 1
  }
}
